{
  "name": "vue-element-admin",
  "version": "5.0.0",
  "description": "A magical vue admin. An out-of-box UI solution for enterprise applications. Newest development stack of vue. Lots of awesome features",
  "author": "Pan <panfree23@gmail.com>",
  "scripts": {
    "serve": "node ./script/app.js --max-old-space-size=100000",
    "dev": "rsbuild dev",
    "dev:test": "cross-env NODE_OPTIONS=--max_old_space_size=8192 rsbuild dev",
    "dev:beta": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=beta rsbuild dev --mode development",
    "dev:prod": "vue-cli-service serve --mode production",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit",
    "lint": "eslint --ext .js,.vue src",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "build:beta": "vue-cli-service build --mode beta",
    "build:dev": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode test",
    "preview": "node build/index.js --preview",
    "new": "plop",
    "postinstall": "patch-package",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "prepare": "husky install",
    "dev:setTest": "set NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=9000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode test",
    "dev:setDev": "set NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=9000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode development",
    "dev:setBeta": "set NODE_OPTIONS=--openssl-legacy-provider && node --max_old_space_size=9000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode beta"
  },
